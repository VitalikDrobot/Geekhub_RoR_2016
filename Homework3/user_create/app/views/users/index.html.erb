<h1>All users</h1>
<%= link_to 'Add new user', new_user_path %>
<% @user.each do |user| %>
<ul>
	<li>Users email</li>
	<li><%= user.email %></li>
	<li><%= link_to 'Show', user_path(user) %></li>
	<li><%= link_to 'Edit', edit_user_path(user) %></li>
	<li><%= link_to 'Destroy', user_path(user),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></li>
</ul>


<% end %>	